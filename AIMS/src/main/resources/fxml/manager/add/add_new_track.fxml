<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox fx:id="hboxTrack" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="newTrack" prefHeight="26.0" prefWidth="338.0" promptText="New track" />
      <Pane prefHeight="26.0" prefWidth="94.0" />
      <Button mnemonicParsing="false" onAction="#deleteTrack" text="Delete" />
   </children>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</HBox>
